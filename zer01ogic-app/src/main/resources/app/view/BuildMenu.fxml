<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="gridpane_menu_buildmenu" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.172-ea"
          xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="-Infinity"/>
        <ColumnConstraints hgrow="NEVER"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <ButtonBar fx:id="buttonbar_menu_toolbar" buttonMinWidth="40.0" buttonOrder="U+FBIX_NCYOA_R" prefHeight="50.0">
            <buttons>
                <Button fx:id="button_menu_toolbar_selection" alignment="CENTER" maxHeight="40.0" maxWidth="40.0"
                        minHeight="40.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
                    <graphic>
                        <ImageView fx:id="imageview_menu_toolbar_selection" fitHeight="34.0" fitWidth="34.0"
                                   pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../graphics/Toolbar_Icons/Selection_tool.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="button_menu_toolbar_wire" alignment="CENTER" maxHeight="40.0" maxWidth="40.0"
                        minHeight="40.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
                    <graphic>
                        <ImageView fx:id="imageview_menu_toolbar_wire" fitHeight="34.0" fitWidth="34.0"
                                   pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../graphics/Toolbar_Icons/Wire_tool.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="button_menu_toolbar_eraser" alignment="CENTER" maxHeight="40.0" maxWidth="40.0"
                        minHeight="40.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
                    <graphic>
                        <ImageView fx:id="imageview_menu_toolbar_eraser" fitHeight="34.0" fitWidth="34.0"
                                   pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../graphics/Toolbar_Icons/Eraser_tool.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="button_menu_toolbar_hand" alignment="CENTER" maxHeight="40.0" maxWidth="40.0"
                        minHeight="40.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
                    <graphic>
                        <ImageView fx:id="imageview_menu_toolbar_hand" fitHeight="34.0" fitWidth="34.0"
                                   pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../graphics/Toolbar_Icons/Hand_tool.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="button_menu_toolbar_pointer" alignment="CENTER" maxHeight="40.0" maxWidth="40.0"
                        minHeight="40.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
                    <graphic>
                        <ImageView fx:id="imageview_menu_toolbar_pointer" fitHeight="34.0" fitWidth="34.0"
                                   pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../graphics/Toolbar_Icons/Pointer_tool.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </buttons>
            <padding>
                <Insets left="10.0" right="10.0"/>
            </padding>
        </ButtonBar>
        <ButtonBar fx:id="buttonbar_menu_mode" buttonMinWidth="50.0" buttonOrder="U+FBIX_NCYOA_R" prefHeight="50.0"
                   GridPane.columnIndex="1">
            <buttons>
                <Button fx:id="button_menu_mode_simulation" maxHeight="48.0" maxWidth="48.0" minHeight="48.0"
                        minWidth="48.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="48.0">
                    <graphic>
                        <ImageView fx:id="imageview_menu_mode_build" fitHeight="44.0" fitWidth="44.0"
                                   pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../graphics/Mode_Icons/Simulation_mode_deselected.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="button_menu_mode_build" maxHeight="48.0" maxWidth="48.0" minHeight="48.0" minWidth="48.0"
                        mnemonicParsing="false" prefHeight="48.0" prefWidth="48.0">
                    <graphic>
                        <ImageView fx:id="image_menu_mode_build" fitHeight="44.0" fitWidth="44.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../../graphics/Mode_Icons/Build_mode_selected.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </buttons>
            <padding>
                <Insets left="10.0" right="10.0"/>
            </padding>
        </ButtonBar>
    </children>
</GridPane>
